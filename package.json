{
  "name": "insert-end",
  "displayName": "Insert End",
  "description": "Automatically inserts 'end' in Ruby code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "insert-end.insertEnd",
        "title": "Insert End Automatically"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linkalls/insert-end.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p tsconfig.json",
    "watch": "tsc -watch -p tsconfig.json",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "typescript": "^5.6.2",
    "vscode-test": "^1.6.0"
  }
}
